{
  "name": "mwd-theme",
  "version": "2.1.0",
  "description": "Theme built around automation with gulp & parcel",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"gulp\" \"rm -rf .cache && parcel library/js/scripts.js --out-dir library/js/dist --out-file scripts.min.js\"",
    "build": "parcel build library/js/scripts.js --out-dir library/js/dist --out-file scripts.min.js && gulp build"
  },
  "author": "AJ C",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime-corejs2": "^7.10.2",
    "autoprefixer": "^8.6.5",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.1",
    "concurrently": "^5.2.0",
    "gulp-babel": "^8.0.0",
    "gulp-image": "^6.1.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-uglify": "^3.0.2",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "gulp": "^4.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "not ie <= 10"
  ]
}
